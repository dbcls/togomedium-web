{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "paths": {
      "%viewer/*": ["../viewer/src/*"],
      "%core/*": ["../core/src/*"],
      "%editor/*": ["../editor/src/*"],
      "%schema/*": ["../schema/src/*"],
    }
  },
  "include": [
    "vite.config.ts"
  ]
}
